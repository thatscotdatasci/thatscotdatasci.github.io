---
interface Props {
  className?: string;
}

const { className = 'w-8 h-8' } = Astro.props;
---

<svg 
  viewBox="0 0 100 100" 
  class={className}
  xmlns="http://www.w3.org/2000/svg"
>
  <rect
    x="10"
    y="10"
    width="80"
    height="80"
    fill="none"
    stroke="#f97316"
    stroke-width="3"
    rx="4"
  />
  <text
    x="50"
    y="50"
    dominant-baseline="central"
    text-anchor="middle"
    fill="#f97316"
    font-size="42"
    font-family="Arial, sans-serif"
    font-weight="700"
  >
    AC
  </text>
</svg>

